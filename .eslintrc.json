{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "quotes": ["error", "double"],
    "max-len": ["error", { "ignoreComments": true, "ignoreUrls": true }],
    "brace-style": ["error", "allman"]
  }
}
